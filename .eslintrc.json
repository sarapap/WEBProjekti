{
    "env": {
        "node": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "google",
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": 0,
        "new-cap": 0,
        "camelcase": 0
    }
}